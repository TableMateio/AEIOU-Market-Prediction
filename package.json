{
    "name": "aeiou-market-prediction",
    "version": "1.0.0",
    "description": "News-driven market prediction engine that models investor belief systems",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register -r dotenv/config src/index.ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "test-newsapi-ai": "npx tsx src/scripts/test-newsapi-ai.ts",
        "test-alpaca": "npx tsx src/scripts/test-alpaca-stock.ts",
        "test-tiingo": "npx tsx src/scripts/test-tiingo-stock.ts",
        "compare-apis": "npx tsx src/scripts/compare-stock-apis.ts",
        "test-all-apis": "npm run test-newsapi-ai && npm run test-alpaca && npm run test-tiingo && npm run compare-apis",
        "collect-benchmarks": "npx tsx src/scripts/ml/collectBenchmarkData.ts",
        "train-ml": "npx tsx src/scripts/ml/trainMLPipeline.ts",
        "setup-python": "cd python && python -m venv venv && source venv/bin/activate && pip install -r requirements.txt",
        "python-train": "cd python && python train_random_forest.py",
        "shap-analysis": "cd python && python shap_analysis.py",
        "analyze-ml-data": "npx tsx src/scripts/ml/analyzeCurrentMLData.ts",
        "export-ml-data": "npx tsx src/scripts/ml/exportMLTrainingData.ts",
        "run-actual-ml": "npx tsx src/scripts/ml/runActualMLPipeline.ts"
    },
    "keywords": [
        "market-prediction",
        "news-analysis",
        "causal-chains",
        "financial-data",
        "investor-psychology"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@anthropic-ai/sdk": "^0.60.0",
        "@felores/airtable-mcp-server": "^0.3.0",
        "@supabase/supabase-js": "^2.56.0",
        "@types/cheerio": "^0.22.35",
        "@types/jsdom": "^21.1.7",
        "@types/node-fetch": "^2.6.13",
        "@types/uuid": "^10.0.0",
        "airtable": "^0.12.2",
        "axios": "^1.11.0",
        "cheerio": "^1.1.2",
        "cors": "^2.8.5",
        "dotenv": "^16.6.1",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "finlight-client": "^2.2.4",
        "helmet": "^7.1.0",
        "joi": "^17.11.0",
        "jsdom": "^26.1.0",
        "node-cron": "^3.0.3",
        "openai": "^5.15.0",
        "tsconfig-paths": "^4.2.0",
        "uuid": "^11.1.0",
        "winston": "^3.11.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.8",
        "@types/node": "^20.10.5",
        "@typescript-eslint/eslint-plugin": "^6.14.0",
        "@typescript-eslint/parser": "^6.14.0",
        "eslint": "^8.55.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.3.3"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}